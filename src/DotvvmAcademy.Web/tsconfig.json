{
  "compilerOptions": {
    "outFile": "wwwroot/scripts/app.js",
    "lib": [ "dom", "es5", "es2015.core", "es2015.promise" ],
    "strictNullChecks": false,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}

{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "strict": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "ES6",
    "strictNullChecks": false,
    "lib": [ "DOM", "ES5", "ES2015.Promise", "ES6" ],
    "module": "ES6",
    "types": [ "knockout" ],
    "moduleResolution": "node",
    "outDir": "../../wwwroot/scripts",
    "typeRoots": [
      "../../node_modules/@types",
      "../../node_modules/dotvvm-types",
      "../../node_modules/monaco-editor"
    ]
  },
  "compileOnSave": false,
  "include": [
    "**/*.ts",
    "../../node_modules/dotvvm-types/DotVVM.d.ts",
    "../../node_modules/monaco-editor/monaco.d.ts"
    //"../Localization/UILanguage.cs"
  ] 
}
